{
  "name": "@friday-team/icons",
  "version": "0.0.0-development",
  "description": "Design System UI Icons",
  "main": "index.js",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/malmo-east/icons.git"
  },
  "bugs": {
    "url": "https://github.com/malmo-east/icons/issues"
  },
  "homepage": "https://github.com/malmo-east/icons#readme",
  "scripts": {
    "build": "chmod +x ./bin/build.sh",
    "clean": "rimraf packages/*/{tsconfig.tsbuildinfo,dist} && rimraf {dist,build}",
    "format-icons": "prettier --write \"./packages/**/*.{ts,tsx}\" --loglevel warn",
    "lint": "eslint ./scripts/ --ext .ts,.tsx",
    "cm": "git-cz",
    "pub:ci": "chmod +x ./bin/publish-ci.sh",
    "postinstall": "node ./send-stats.js > /dev/null 2>&1 || exit 0",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "@rollup/plugin-json": "^4.1.0",
    "@wessberg/rollup-plugin-ts": "^2.0.4"
  },
  "devDependencies": {
    "@rollup/plugin-auto-install": "^2.0.0",
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@rollup/plugin-typescript": "^4.0.0",
    "@types/camelcase": "^5.2.0",
    "@types/node": "^13.9.1",
    "@types/react": "^16.9.23",
    "@types/svgo": "^1.3.1",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/parser": "^2.24.0",
    "camelcase": "^5.3.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-chai-friendly": "^0.5.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.0",
    "eslint-plugin-sort-class-members": "^1.6.0",
    "cz-conventional-changelog": "^3.3.0",
    "lerna": "^3.20.2",
    "path": "^0.12.7",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.1.0",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-multi-input": "^1.1.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-terser": "^5.3.0",
    "stylelint": "^13.2.1",
    "svgo": "^1.3.2",
    "ts-node": "^8.6.2",
    "tslib": "^2.0.0",
    "typescript": "^3.8.3",
    "util": "^0.12.2",
    "semantic-release": "^19.0.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "concurrent": true,
    "linters": {
      "scripts/**/*.{js,jsx,ts,tsx}": [
        "prettier --write",
        "git add",
        "eslint"
      ]
    }
  }
}
